<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <defaults autowire="false" autoconfigure="false"/>
        <prototype namespace="App\" resource="../src/" exclude="../src/{DependencyInjection,Entity,Kernel.php}"/>

        <service id="app.orm.entity_manager" class="App\Service\ORM\EntityManager" lazy="true">
            <argument type="service" id="doctrine.orm.entity_manager"/>
            <argument type="service" id="oa.dynamodb.entity_manager"/>
        </service>
    </services>
</container>