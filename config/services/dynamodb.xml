<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        https://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="app.dynamodb_schema_extract_command"
                 class="App\Command\Dynamodb\DynamodbSchemaExtractCommand" lazy="true">
            <tag name="console.command" command="dynamodb:schema:extract"/>
        </service>
        <service id="app.dynamodb_fixtures_load_command"
                 class="App\Command\Dynamodb\DynamodbFixturesLoadCommand" lazy="true">
            <tag name="console.command" command="dynamodb:fixtures:load"/>
        </service>
        <service id="app.dynamodb_from_doctrine_transfer_command"
                 class="App\Command\Dynamodb\DynamodbFromDoctrineTransferCommand" lazy="true">
            <argument type="service" id="app.orm.entity_manager"/>
            <argument type="service" id="app.telegram_bot_repository"/>
            <argument type="service" id="app.telegram_channel_repository"/>
            <argument type="service" id="app.level_1_region_repository"/>
            <argument type="service" id="app.user_repository"/>
            <argument type="service" id="app.telegram_bot_creator"/>
            <argument type="service" id="app.telegram_channel_creator"/>
            <argument type="service" id="app.intl_country_provider"/>
            <argument type="service" id="app.intl_locale_provider"/>
            <argument type="service" id="app.level_1_region_provider"/>
            <argument type="service" id="app.id_generator"/>
            <tag name="console.command" command="dynamodb:from-doctrine:transfer"/>
        </service>
    </services>
</container>